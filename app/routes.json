{
    "routes": [
        {
            "route": "/api/setRecipie",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/api/recipie",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "platformErrorOverrides": [
        {
            "errorType": "NotFound",
            "serve": "/static/404.html"
        },
        {
            "errorType": "Unauthenticated",
            "serve": "/static/login.html"
        }
    ]
}